{
  "name": "hotel-frontend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "hotel-frontend",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/hotel-frontend"
      },
      "configurations": {
        "development": {
          "outputPath": "hotel-frontend"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "hotel-frontend:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "hotel-frontend:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "hotel-frontend:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "hotel-frontend:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "hotel-frontend/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["hotel-frontend/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
